{
  "expo": {
    "name": "DiaMate",
    "slug": "diamate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#667eea"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.diamate.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSHealthShareUsageDescription": "DiaMate, diyabet yönetiminizi kişiselleştirmek için sağlık verilerinizi okur.",
        "NSHealthUpdateUsageDescription": "DiaMate, öğün ve insülin kayıtlarınızı Apple Health'e yazabilir.",
        "NSCameraUsageDescription": "DiaMate, yemek fotoğraflarını analiz etmek için kameranızı kullanır.",
        "NSPhotoLibraryUsageDescription": "DiaMate, yemek fotoğraflarını analiz etmek için fotoğraflarınıza erişir.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "com.apple.developer.healthkit": true,
        "com.apple.developer.healthkit.background-delivery": true
      },
      "associatedDomains": [
        "applinks:diamate.org",
        "webcredentials:diamate.org",
        "applinks:diamate.org",
        "webcredentials:diamate.org"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#667eea"
      },
      "package": "org.diamate.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-camera",
        {
          "cameraPermission": "DiaMate yemek fotoğraflarını analiz etmek için kameranızı kullanır."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "DiaMate yemek fotoğraflarını analiz etmek için fotoğraflarınıza erişir."
        }
      ],
      [
        "react-native-health",
        {
          "isClinicalDataEnabled": false
        }
      ]
    ],
    "extra": {
      "apiUrl": "https://diamate.org/.netlify/functions",
      "eas": {
        "projectId": "42a5b3b7-6185-4cec-a94b-44b7f30d1368"
      }
    },
    "scheme": "diamate",
    "owner": "berken1dogan"
  }
}
