{
  "name": "diamate-mobile",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build:ios": "eas build --platform ios --profile preview",
    "build:android": "eas build --platform android --profile preview",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@supabase/supabase-js": "^2.49.4",
    "date-fns": "^3.0.0",
    "expo": "~53.0.0",
    "expo-build-properties": "~0.14.5",
    "expo-camera": "~16.1.0",
    "expo-constants": "~17.1.0",
    "expo-haptics": "~14.1.0",
    "expo-image-picker": "~16.1.0",
    "expo-linking": "~7.1.0",
    "expo-secure-store": "~14.2.4",
    "expo-status-bar": "~2.2.0",
    "react": "19.0.0",
    "react-native": "0.79.6",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-health": "^1.18.0",
    "react-native-iap": "^12.15.0",
    "react-native-reanimated": "~3.17.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@types/react": "~19.0.10",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "patch-package": "^8.0.1",
    "typescript": "~5.8.3"
  },
  "private": true,
  "overrides": {
    "@expo/config-plugins": "~10.1.1"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-health"
        ]
      }
    }
  }
